let rec length S =
S eq '' -> 0 | 1 + length (Stern S)
in
let Substring s n m =
not (Isstring s) -> 'Not String' | n gr length(s) -> 'Out_Of_Range' | n+m gr length(s) -> 'Out_Of_Range' |
Check_String (s,n,n+m,1)
where rec Check_String(s,r,l,c) =
c ge l -> '' | c ge r ->  Conc (Stem s)  (Check_String(Stern s, r, l, c+1))  | Check_String(Stern s, r, l, c+1)
in Print(Substring 'adjshf' 3 2)

